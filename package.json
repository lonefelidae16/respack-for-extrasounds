        "file-saver": "^2.0.5",
        "html-loader": "^4.2.0",
        "html-webpack-plugin": "^5.5.2",
        "husky": "^8.0.3",
        "jquery": "^3.7.0",
        "jszip": "^3.10.1",
        "lint-staged": "^13.2.2",
        "mini-css-extract-plugin": "^2.7.6",
        "peer": "^1.0.0",
        "rimraf": "^5.0.1",
        "sass": "^1.63.3",
        "sass-loader": "^13.3.2",
        "stylelint": "^15.7.0",
